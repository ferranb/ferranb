<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_height="fill_parent" android:layout_width="fill_parent">
    <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:layout_width="fill_parent" android:orientation="horizontal">
        <ToggleButton android:layout_height="wrap_content" android:text="ToggleButton" android:id="@+id/GPSButton" android:textOn="@string/gps_button" android:textOff="@string/gps_button" android:layout_weight="1" android:layout_width="0dp"></ToggleButton>
        <ToggleButton android:layout_height="wrap_content" android:text="ToggleButton" android:id="@+id/NetworkButton" android:textOn="@string/network_button" android:textOff="@string/network_button" android:layout_weight="1" android:layout_width="0dp"></ToggleButton>
        <Button android:id="@+id/LastLocationButton" android:layout_height="fill_parent" android:layout_gravity="center_vertical" android:layout_weight="1" android:layout_width="0dp"></Button>
    </LinearLayout>
    <EditText android:layout_width="fill_parent" android:inputType="textMultiLine" android:layout_height="fill_parent" android:id="@+id/text">
        <requestFocus></requestFocus>
    </EditText>
</LinearLayout>
